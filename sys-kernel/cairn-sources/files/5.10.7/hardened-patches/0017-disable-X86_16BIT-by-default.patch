 arch/x86/Kconfig | 1 -
 1 file changed, 1 deletion(-)

diff --git a/arch/x86/Kconfig b/arch/x86/Kconfig
index ac5c142ce1e7..f01a75a8d6b1 100644
--- a/arch/x86/Kconfig
+++ b/arch/x86/Kconfig
@@ -1194,7 +1194,6 @@ config VM86
 
 config X86_16BIT
 	bool "Enable support for 16-bit segments"
