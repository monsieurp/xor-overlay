---
 arch/x86/Kconfig | 1 -
 1 file changed, 1 deletion(-)

diff --git a/arch/x86/Kconfig b/arch/x86/Kconfig
index be083b897ad9..89a3be1256fe 100644
--- a/arch/x86/Kconfig
+++ b/arch/x86/Kconfig
@@ -1188,7 +1188,6 @@ config VM86
 
 config X86_16BIT
 	bool "Enable support for 16-bit segments"
