Subject: [PATCH 046/110] disable UID16 by default

---
 init/Kconfig | 1 -
 1 file changed, 1 deletion(-)

diff --git a/init/Kconfig b/init/Kconfig
index e863f3f4c39f..401cb32a8006 100644
--- a/init/Kconfig
+++ b/init/Kconfig
@@ -1410,7 +1410,6 @@ menuconfig EXPERT
 config UID16
 	bool "Enable 16-bit UID system calls"
 	depends on HAVE_UID16 && MULTIUSER
