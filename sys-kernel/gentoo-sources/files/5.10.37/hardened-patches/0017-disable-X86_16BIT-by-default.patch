 arch/x86/Kconfig | 1 -
 1 file changed, 1 deletion(-)

diff --git a/arch/x86/Kconfig b/arch/x86/Kconfig
index 77734859ed2c..b72654d2a7f9 100644
--- a/arch/x86/Kconfig
+++ b/arch/x86/Kconfig
@@ -1196,7 +1196,6 @@ config VM86
 
 config X86_16BIT
 	bool "Enable support for 16-bit segments"
